<functions xmlns="http://software.in2p3.fr/lavoisier/functions.xsd" >
    <function name="apos" eval='concat("&apos;", $string, "&apos;")'>
        <argument>string</argument>
    </function>
    <function name="quot" eval="concat('&quot;', $string, '&quot;')">
        <argument>string</argument>
    </function>
    <function name="error_percentage" eval="(($num1 - $num2) div ($num1 + $num2)) *200">
        <argument>num1</argument>
        <argument>num2</argument>
    </function>

    <function name="arround" eval="(round($num*100)) div 100">
        <argument>num</argument>
    </function>
</functions>